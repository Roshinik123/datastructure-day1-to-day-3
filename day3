#include <stdio.h>
int main() {
    int count=0;
    char arr[6]={'r','o','s','h','4','7'};
//printf("enter the pasword");
for(int i=0;i<6;i++)
{
    if(arr[i]>='0'&&arr[i]<='9')
    count++;
}
for(int i=0;i<6;i++)
{
    printf("%c",arr[i]);
}
printf("\ndigit=%d",count);
}
output;
rosh47
digit=2
