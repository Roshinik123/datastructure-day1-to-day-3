#include <stdio.h>
int main() {
    int i,n;
    printf("enter the number");
    scanf("%d",&n);
   int arr[n];
  for(i=0;i<n;i++)
  {
      scanf("%d",&arr[i]);
  }
  for(i=0;i<n;i++)
  {
      printf("%d",arr[i]);
  }
    return 0;
}
output;
enter the number5
1
2
3
4
5
12345
