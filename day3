Day 1
1)#include <stdio.h>
int main()
{
    int a,b;
    printf("enter a valid integer");
    scanf("%d",&a);
    printf("enter a valid integer");
    scanf("%d",&b);
    //printf("a=%d & b=%d",a,b);
    printf("the value of %d+%d is %d",a,b,a+b);
    printf("\nthe value of %d-%d is %d",a,b,a-b);
    printf("\nthe value of %d*%d is %d",a,b,a*b);
    printf("\nthe value of %d/%d is %d",a,b,a/b);
    printf("\nthe value of %dR%d is %d",a,b,a%b);
    return 0;
}

2)#include <stdio.h>
int main()
{
    int a,b;
    printf("enter a valid integer");
    scanf("%d",&a);
    printf("enter a valid integer");
    scanf("%d",&b);
    printf("a=%d & b=%d",a,b);
return 0;
}
3)#include <stdio.h>
int main()
{
    int a,b,h;
    printf("enter the value",a,b,h);
    scanf("%d %d %d",&a,&b,&h);
printf("area of triangle %d \n perimeter of triangle %d",(b*h)/2,(a+b+h));
    return 0;
}
Day 2
1)#include <stdio.h>
int main()
{
    int i,j,n;
    printf("enter the maximum no of the element");
    scanf("%d",&n);
    for(i=n;i>0;i--)
    {
    for(j=1;j<=i;j++)
    {
        printf("*");
       }
       printf("\n");
       }
       return 0;
}
output;
enter the maximum no of the element5
*****
****
***
**
*

2)#include <stdio.h>
int main()
{
    int i,j,n;
    printf("enter the maximum no of the element");
    scanf("%d",&n);
    for(i=1;i<n;i++)
    {
    for(j=1;j<=n;j++)
    {
        printf("%d",i);
       }
       printf("\n");
       }
       return 0;
}
