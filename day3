int main() {
    int i,n,sum=0;
    printf("enter the number");
    scanf("%d",&n);
   int arr[n];
  for(i=0;i<n;i++)
  {
      scanf("%d",&arr[i]);
  }
  for(i=0;i<n;i++)
  {
      sum=sum+arr[i];
  }
   printf("%d",sum);
    return 0;
}
output;
