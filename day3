1)
#include <stdio.h>
#include<string.h>
int main() {
char s1[20],s2[20];
printf("enter  the string I");
gets(s1);
printf("enter  the string II");
gets(s2);
printf("string before concatenation s1=%s s2=%s",s1,s2);
strcpy(s2,s1);
printf("\nstring after concatenation s1=%ss2=%s",s1,s2);
    return 0;
}
output;
enter  the string Iroshini
enter  the string IIsuresh
string before concatenation s1=roshini s2=suresh
string after concatenation s1=roshinis2=roshini
2)
// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() {
    char ch[20];
    gets(ch);
    puts(ch);
    int l=strlen(ch);
    printf("string length=%d",l);

    return 0;
}
output;
roshini suresh
roshini suresh
string length=14
3)#include <stdio.h>
#include<string.h>
int main() {
    char s1[20],s2[20];
    gets(s1);
    gets(s2);
    printf("s1=%s s2=%s",s1,s2);
    int r=strcmp(s1,s2);
    if(r==0)
    printf("both are equal");
    else if(r>0)
    printf("\ns1 is bigger");
    else
    printf("\ns2 is bigger");

    return 0;
}
output;
roshini
vikashini
s1=roshini s2=vikashini
s2 is bigger
