4)#include <stdio.h>
int main()
{
    int i,j,k,n;
    printf("enter the maximum no of the element");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
    for(j=1;j<=2*n-i;j++)
    {
        printf(" ");
       }
       for(k=1;k<=i;k++)
       printf("* ");
       printf("\n");
       }
       return 0;
}
output;
enter the maximum no of the element5
         * 
        * * 
       * * * 
      * * * * 
     * * * * * 
