#include <stdio.h>
int main() {
    int i,j,n1,n2;
    printf("enter the 2d array[r] [c]");
    scanf("%d %d",&n1,&n2);
   int arr1[n1][n2],arr2[n1][n2],div[n1][n2];
   printf("enter the elements");
  for(i=0;i<n1;i++)
  {
 for(j=0;j<n2;j++) 
  {
      scanf("%d",&arr1[i][j]);
  }
  }
for(i=0;i<n1;i++)
  {
  for(j=0;j<n2;j++)
{
printf("%2d",arr1[i][j]);  
}
printf("\n");
}
for(i=0;i<n1;i++)

 {
 for(j=0;j<n2;j++) 
  {
      scanf("%d",&arr2[i][j]);
  }
  }
for(i=0;i<n1;i++)
  {
  for(j=0;j<n2;j++)
{
printf("%2d",arr2[i][j]);  
}
printf("\n");
}
for(i=0;i<n1;i++)
  {
  for(j=0;j<n2;j++)
{
div[i][j]=arr1[i][j]/arr2[i][j];  
}
}
for(i=0;i<n1;i++)
  {
  for(j=0;j<n2;j++)
{
printf("%2d",div[i][j]);
}
printf("\n");
}
}
output;

 enter the 2d array[r] [c]2
2
enter the elements5 6 7 8
 5 6
 7 8
1 2 3 4
 1 2
 3 4
 5 3
 2 2
