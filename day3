3)
#include <stdio.h>
int main() {
    int i,n,res=1;
    printf("enter the number");
    scanf("%d",&n);
   int arr[n];
  for(i=0;i<n;i++)
  {
      scanf("%d",&arr[i]);
  }
  for(i=0;i<n;i++)
  {
      res=res*arr[i];
  }
   printf("product=%d",res);
    return 0;
}
output;
enter the number3
1
2
3
product=6
4)
#include <stdio.h>
int main() {
    int i,n;
    printf("enter the number");
    scanf("%d",&n);
   int arr[n];
  for(i=0;i<n;i++)
  {
      scanf("%d",&arr[i]);
  }
  int min=arr[0],max=arr[0];
  for(i=0;i<n;i++)
  {
  if(min>arr[i])
 {
     min=arr[i];
 }
 else if(max<arr[i])
 {
     max=arr[i];
 }
  }
   printf("min=%d\nmax=%d",min,max);
    return 0;
}
output
enter the number5
1
2
3
4
5
min=1
max=5

