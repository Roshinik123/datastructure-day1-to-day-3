1)#include <stdio.h>
int main()
{
    int i,j,n;
    printf("enter the maximum no of the element");
    scanf("%d",&n);
    for(i=n;i>0;i--)
    {
    for(j=1;j<=i;j++)
    {
        printf("*");
       }
       printf("\n");
       }
       return 0;
}
output;
enter the maximum no of the element5
*****
****
***
**
*

2)#include <stdio.h>
int main()
{
    int i,j,n;
    printf("enter the maximum no of the element");
    scanf("%d",&n);
    for(i=1;i<n;i++)
    {
    for(j=1;j<=n;j++)
    {
        printf("%d",i);
       }
       printf("\n");
       }
       return 0;
}
output;
